la(105, 46%, 56%, 0.4) 79.9%, hsla(105, 46%, 56%, 0.2) 80%);\n\n$bg-good: #38984D;\n$bg-good-light: hsla(105, 46%, 56%, 1);\n$gradient-good: linear-gradient(0deg, hsla(105, 46%, 56%, 1) 0%, hsla(105, 46%, 56%, 1) 19.9%, hsla(105, 46%, 56%, 0.8) 20%, hsla(105, 46%, 56%, 0.8) 39.9%, hsla(105, 46%, 56%, 0.6) 40%, hsla(105, 46%, 56%, 0.6) 59.9%, hsla(105, 46%, 56%, 0.4) 60%, hsla(105, 46%, 56%, 0.4) 79.9%, hsla(105, 46%, 56%, 0.2) 80%);\n\n$bg-bad: hsla(9,100,61,1);\n$bg-bad-light: hsla(29,100,63,1);\n$gradient-bad: linear-gradient(0deg, hsla(29,100,63, 1) 0%, hsla(29,100,63, 1) 19.9%, hsla(29,100,63, 0.8) 20%, hsla(29,100,63, 0.8) 39.9%, hsla(29,100,63, 0.6) 40%, hsla(29,100,63, 0.6) 59.9%, hsla(29,100,63, 0.4) 60%, hsla(29,100,63, 0.4) 79.9%, hsla(29,100,63, 0.2) 80%);\n\n$base-padding: 10px;\n$text_color: white;\n\n$status-height: 5vh;\n$font-base_size:16pt;\n\n\n\n@font-face {\n    font-family: 'NeuePixelGrotesk';\n    src: url('fonts/NeuePixelGrotesk.woff2') format('woff2'),\n        url('fonts/NeuePixelGrotesk.woff') format('woff');\n    font-weight: normal;\n    font-style: normal;\n    font-display: block;\n}\n\n@font-face {\n    font-family: 'AprilSans-Regular';\n    src: url('fonts/AprilSans-Regular.woff2') format('woff2'),\n        url('fonts/AprilSans-Regular.woff') format('woff');\n    font-weight: normal;\n    font-style: normal;\n    font-display: block;\n}\n\n@mixin font-text($_color: $text_color) {\n    font-family: 'AprilSans-Regular';\n    color: $_color;\n}\n\n@mixin font-numbers($_color: $text_color) {\n    font-family: 'NeuePixelGrotesk';\n    color: $_color;\n}","css/app.scss":"\n@use 'theme';\n\nbody {\n    margin:0px;\n    padding: 0px;\n   // overflow: hidden;\n}\n\n#app {\n    background-color: theme.$bg-default;\n    @include theme.font-text;\n    height: 100vh;\n    font-size: theme.$font-base_size;\n}\n\nsection.statusbar {\n    display: flex;\n    // border: 1px solid red;\n    width:100%;\n    padding: theme.$base-padding;\n    flex-direction: row;\n    flex-flow: space-between;\n    align-items: center;\n    height: theme.$status-height;\n    position: absolute;\n    z-index: 10;\n    \n    ul {\n        margin: 0px;\n        margin-right: 20px;\n        padding: 0px;\n        width: 100%;\n        list-style-type: none;\n        display: flex;\n        align-content: stretch;\n\n        li {\n            flex-grow: 1;\n            height:5px;\n            background-color: black;\n            margin-right: 2px;\n            opacity: .1;\n\n            &.done {\n                opacity: .5;\n            }\n\n            &.active {\n                opacity: 1;\n            }\n        }\n    }\n\n    div {\n        // width: 80px;\n        display: flex;\n        align-items: center;\n        \n\n        img.coin {\n            \n            width: 25px;\n            display: inline-block;\n        }\n\n        span {\n            display: block;\n            @include theme.font-numbers;\n            font-size: 80%;\n            text-align: right;\n            \n        }\n    }\n\n\n}\n\nsection.screen {\n    display: flex;\n    flex-direction: column;\n    \n    background: theme.$bg-default;\n    padding-top: theme.$status-height;\n    height:100%;\n    width:100%;\n    z-index: 1;\n    position: fixed;\n    align-items: flex-start;\n    \n\n    div.text_wrapper {\n        flex-grow: 2;\n        display: flex;\n        flex-direction: column;\n        width:100%;\n    }\n   \n    \n    img.cover {\n        // padding: theme.$base-padding;\n\n        width:100%;\n        margin:auto;\n        max-height: 40vh;\n        object-fit: cover;\n        image-rendering: pixelated;\n    }\n\n    h1 {\n        padding-left: theme.$base-padding;\n        padding-right: theme.$base-padding;\n        margin:auto;\n        font-size:150%;\n        font-weight: normal;\n        width: 90%;\n        font-variant-numeric: tabular-nums lining-nums;\n\n        img {\n            display:none;\n        }\n       \n    }\n\n    div.text {\n        padding-left: theme.$base-padding;\n        padding-right: theme.$base-padding;\n        flex-grow: 2;\n        margin:auto;\n        margin-top:30px;\n        width: 90%;\n        line-height: 100%;\n        display: flex;\n        flex-direction: column;\n        justify-content: flex-start;\n\n        .nes-container {\n            background-color: transparent;\n\n            h3.title {\n                background-color: theme.$bg-default;\n                margin-top: -1.8rem !important;\n                font-size: 75%;\n                margin-bottom: 0px;\n            }\n        }\n    }\n\n\n\n    &.positive  {\n        background: theme.$bg-good;\n\n        div.gradient {\n            background: theme.$gradient-good;\n        }\n\n        h1 {\n            img {\n                display: inline-block;\n                width:15vw;\n            }\n            font-size: 400%;\n            text-align: center;\n        }\n\n        .nes-container\n\n        div.buttons {background: theme.$bg-good-light;}\n    }\n\n    &.negative  {\n\n        background: theme.$bg-bad;\n\n        div.gradient {\n            background: theme.$gradient-bad;\n        }\n\n        h1 {\n            img {\n                display: inline-block;\n                width:15vw;\n            }\n            font-size: 400%;\n            text-align: center;\n        }\n\n        div.buttons { background: theme.$bg-bad-light;}\n    }\n\n\n    div.gradient {\n        display: block;\n        background: theme.$gradient-default;\n        height:5vh;\n    }\n\n    div.buttons {\n        display: flex;\n        flex-direction: column;\n        padding: theme.$base-padding;\n        background: theme.$bg-default-light;\n    }\n\n    button {\n        font-size:100%;\n        margin-bottom: 2%;\n        line-height: 90%;\n    }\n\n\n}\n\n@media screen and (min-width: 1024px){\n    \n    section.screen {\n        flex-direction: row;\n        justify-content: center;\n        align-items: center;\n    }\n\n    //flexbox - children layout\n    section.screen img.cover,\n    .text_wrapper {\n        width:30%;\n        max-width: 480px;\n    }\n\n\n    section.screen img.cover {\n        margin: 0;\n        object-fit: contain;\n        margin-right:2.5%;\n    }\n\n    .text_wrapper{\n        border-left: 4px solid rgba(255, 255, 255, 0.7);\n        border-right: 4px solid rgba(255, 255, 255, 0.7);\n        margin-left: 2.5%;\n        position:relative;\n    }\n    \n    //left column fake border top and bottom\n    div.text_wrapper h1 ::before,\n    .button_wrapper ::after{\n        content: ' ';\n        position: absolute;\n        display: block;\n        height: 4px;\n        background-color: rgba(255, 255, 255, 0.7);\n        left: 0px;\n        right: 0px; \n    }\n\n    //right column fake top border position\n   div.text_wrapper h1 ::before {\n       top: -2px;\n   }\n\n    //right column fake bottom border position\n   .button_wrapper ::after {\n       bottom: -4px;\n   }\n\n    //response img size fix (positive, negative)\n    section.screen.positive h1 img,\n    section.screen.negative h1 img{\n        width: 25%;\n        max-width: 100px;\n    }\n    \n}"},"lineCount":null}},"error":null,"hash":"505f3fdf4a3cd5c1554cdf4a8b5a1c1d","cacheData":{"env":{}}}